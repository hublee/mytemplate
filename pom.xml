	<properties>
		<!-- version setting -->
		<spring.version>4.1.4.RELEASE</spring.version>
		<beetl.version>2.2.1</beetl.version>
		<pagehelper.version>3.6.4</pagehelper.version>
		<mapper.version>2.3.1</mapper.version>
