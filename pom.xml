			<systemPath>${basedir}/src/main/webapp/WEB-INF/lib/easypoi-base-2.1.1-SNAPSHOT.jar</systemPath>
		</dependency>
		
		<dependency>
			<groupId>org.jeecgframework</groupId>
			<artifactId>easypoi-web</artifactId>
			<version>2.1.1</version>
			<scope>system</scope>
			<systemPath>${basedir}/src/main/webapp/WEB-INF/lib/easypoi-web-2.1.1-SNAPSHOT.jar</systemPath>
